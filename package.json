{
  "scripts": {
    "build": "gulp build",
    "exec": "NODE_PATH=dist node",
    "start": "NODE_PATH=dist node dist/app.js",
    "coverage": "NODE_PATH=src BABEL_ENV=test nyc --check-coverage --lines 95 ava",
    "lint": "node_modules/eslint/bin/eslint.js . --ext .js",
    "test": "NODE_PATH=src BABEL_ENV=test ava"
  },
  "pre-push": [
    "lint",
    "coverage"
  ],
  "dependencies": {
    "bluebird": "^3.5.0",
    "csv": "^1.1.1",
    "d3": "^3.5.17",
    "dimple-js": "^2.1.4",
    "glob": "^7.1.2",
    "image-data-uri": "^1.0.0",
    "lodash": "^4.17.4",
    "phantom": "^4.0.5",
    "phantom-pool": "^1.2.2",
    "plotly.js": "^1.29.3",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "ava": "^0.21.0",
    "babel-core": "^6.25.0",
    "babel-plugin-root-import": "^5.1.0",
    "babel-preset-es2015": "^6.24.1",
    "del": "^3.0.0",
    "eslint": "^4.3.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-cli": "^1.3.0",
    "gulp-shell": "^0.6.3",
    "nyc": "^11.1.0",
    "pre-push": "^0.1.1"
  },
  "nyc": {
    "exclude": [
      "src/plotters",
      "tests",
      "node_modules"
    ]
  },
  "ava": {
    "timeout": "1m",
    "files": [
      "tests/**/*.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "require": "babel-register",
    "babel": {
      "presets": [
        "es2015"
      ],
      "babelrc": true
    }
  },
  "name": "nodeplot",
  "version": "0.0.1",
  "description": "A server-side plotting utility currently focused around phantomjs and plotly",
  "main": "dist/index.js",
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andnp/NodePlot.git"
  },
  "keywords": [
    "plotly",
    "nodejs"
  ],
  "author": "Andy Patterson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andnp/NodePlot/issues"
  },
  "homepage": "https://github.com/andnp/NodePlot#readme"
}
