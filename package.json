{
  "scripts": {
    "exec": "NODE_PATH=dist node",
    "start": "NODE_PATH=dist node dist/app.js",
    "coverage": "NODE_PATH=src BABEL_ENV=test nyc --check-coverage --lines 95 ava",
    "lint": "node_modules/eslint/bin/eslint.js . --ext .js",
    "test": "NODE_PATH=src BABEL_ENV=test ava",
    "postinstall": "gulp copy-node-modules",
    "postuninstall": "gulp copy-node-modules"
  },
  "pre-push": [
    "lint",
    "coverage"
  ],
  "dependencies": {
    "bluebird": "^3.5.0",
    "csv": "^1.1.1",
    "d3-node": "^1.0.3",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "ava": "^0.21.0",
    "babel-core": "^6.25.0",
    "babel-preset-es2015": "^6.24.1",
    "del": "^3.0.0",
    "eslint": "^4.3.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-cli": "^1.3.0",
    "gulp-shell": "^0.6.3",
    "nyc": "^11.1.0",
    "pre-push": "^0.1.1"
  },
  "nyc": {
    "exclude": [
      "tests",
      "node_modules"
    ]
  },
  "ava": {
    "timeout": "1m",
    "files": [
      "tests/**/*.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "require": "babel-register",
    "babel": {
      "presets": [
        "es2015"
      ],
      "babelrc": true
    }
  }
}
